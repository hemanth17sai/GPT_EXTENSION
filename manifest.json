{
  "manifest_version": 3,
  "name": "ChatGPT Question Sidebar (Dev)",
  "version": "0.1",
  "description": "Shows questions in a sidebar",

  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["dist/main.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ]
}
